	<skos:prefLabel xml:lang="en">Judge of</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/judicial-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/submitter-of-legal-petition-to">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between someone seeking a legal ruling and the authority to whom that person submitted a petition.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Submitter of Legal Petition to</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/judicial-relationship"/>
</rdf:Description>

