	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Step Family Relationship</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/qualifier-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/carrier-of-letter-from">
