<rdf:Description rdf:about="http://syriaca.org/taxonomy/churches">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A building for Christian religious services. It can be a parish church, a monastic church, a cathedral, or a small chapel used only occasionally, but not a chapel that is part of a larger church.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
