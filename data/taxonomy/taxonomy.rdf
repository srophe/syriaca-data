	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/fellow-clergy"/>
	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/ordained"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/commemorates">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A "relationship" constituted by a person honoring another in a ritual (civic, religious, etc.); i.e. includes their name in the dyptichs of the church or reads out their name as part of the liturgy.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Commemorates</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/religious-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/commune-together">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship constituted by two or more people partaking together in the Eucharist.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Commune Together</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/religious-relationship"/>
	<skos:related rdf:resource="http://syriaca.org/taxonomy/communion"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/confessor-for">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between someone who hears the confession of another person and that other person</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Confessor for</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/religious-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/monastic-relationship">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between people in a monastic setting</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Monastic Relationship</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/religious-relationship"/>
	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/fellow-monastics"/>
	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/monastic-head-over"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/ritual-kinship">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship with familial significance but established through a ritual act.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Ritual Kinship</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/religious-relationship"/>
	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/godparent-for"/>
	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/brotherhood-with"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/baptized">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between a member of the clergy and a person baptized by that clergy person.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Baptized</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/clerical-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/bishop-over">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between a bishop and a person under his ecclesiastical authority if that person was either laity or whose clerical status is unknown.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Bishop over</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/employer-of"/>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/clerical-relationship"/>
	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/bishop-over-bishop"/>
	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/bishop-over-clergy"/>
	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/bishop-over-monk"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/clergy-for">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between any religious official and a person served by that religious official, not specific to any particular religion; used to describe something like "Person x is my rabbi".</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Clergy for</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/clerical-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/fellow-clergy">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between clergy of the same religious community.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Fellow Clergy</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/colleague-of"/>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/clerical-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/ordained">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between someone performing a rite that bestows religious authority and the person being ordained.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Ordained</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/clerical-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/communion">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Used to describe individuals or parties being in communion with one another; i.e. taking the eucharist together.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Communion</skos:prefLabel>
	<skos:altLabel xml:lang="en">Fellowship</skos:altLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/religion"/>
	<skos:related rdf:resource="http://syriaca.org/taxonomy/commune-together"/>
	<skos:related rdf:resource="http://syriaca.org/taxonomy/eucharist"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/fellow-monastics">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between people living together in monastic community.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Fellow Monastics</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/colleague-of"/>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/monastic-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/monastic-head-over">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between the head of a monastery (abbess/abbot/etc.) and a member of that person's monastery.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Monastic Head over</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/employer-of"/>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/monastic-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/bishop-over-bishop">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between a bishop and a one or more bishops under his ecclesiastical authority, as in a patriarch or metropolitan bishop.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Bishop over Bishop</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/bishop-over"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/bishop-over-clergy">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between a bishop and one or more members of the clergy over whom he exercises authority.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Bishop over Clergy</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/bishop-over"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/bishop-over-monk">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between a bishop and one or more monks over whom he exercises authority.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Bishop over  Monk</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/bishop-over"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/command-over">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship constituted by a person exercising military command over another.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Command over</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/military-relationship"/>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/employer-of"/>
