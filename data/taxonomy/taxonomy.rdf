</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/ancestor-of">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">An ancestor of a person when the relationship is either very distant in time and/or the specific relationship is unknown.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
