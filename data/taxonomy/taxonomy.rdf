	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/cousin-of"/>
	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/descendent-of"/>
	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/sibling-of-parent-of"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/hereditary-family-of">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between parents, children, and siblings.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
