	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Maternal Family Relationship</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/qualifier-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/paternal-family-relationship">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A "qualifier relationship" indicating indicating that a family connection was patrilineal.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Paternal Family Relationship</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/qualifier-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/step-family-relationship">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
