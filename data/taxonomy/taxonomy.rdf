<rdf:Description rdf:about="http://syriaca.org/taxonomy/child-of-sibling-of">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">The child of a sibling of a person, used for an aunt/uncle and neice/nephew relationships.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Child of Sibling of</skos:prefLabel>
