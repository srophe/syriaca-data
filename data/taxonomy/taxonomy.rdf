<rdf:Description rdf:about="http://syriaca.org/taxonomy/military-relationship">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A professional relationship defined by military service.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Military Relationship</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/professional-relationship"/>
	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/command-over"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/patron-of">
