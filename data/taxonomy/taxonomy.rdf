	<rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
	<rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/2002/07/owl#hasValue">
