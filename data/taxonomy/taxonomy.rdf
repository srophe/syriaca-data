</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/family-of">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between two persons who are part of the same family.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Family of</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/kin-of"/>
