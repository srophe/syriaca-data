	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Casual Intimate Relationship with</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/intimate-relationship-with"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/serious-intimate-relationship-with">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A sexual relationship between people this is deemed serious due to duration, level of commitment, etc.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
