	<skos:broader rdf:resource="http://syriaca.org/taxonomy/professional-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/carrier-of-letter-to">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
