	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Sibling of</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/hereditary-family-of"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/grandparent-of">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A parent of one's parent.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Grandparent of</skos:prefLabel>
