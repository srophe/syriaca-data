	<skos:prefLabel xml:lang="en">Acknowledged Family Relationship</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/qualifier-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/adopted-family-relationship">
