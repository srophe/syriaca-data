</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/grandchild-of">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Grandchild of</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/descendent-of"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/acknowledged-family-relationship">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
