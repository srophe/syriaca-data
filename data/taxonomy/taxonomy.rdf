</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/great-grandparent-of">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">The grandparent of one's parent.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
