</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/legally-recognized-relationship-with">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A sexual relationship that is recognized in law</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Legally Recognized Relationship with</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/serious-intimate-relationship-with"/>
	<skos:narrower rdf:resource="http://syriaca.org/taxonomy/spouse-of"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/spouse-of">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between persons with a legally and/or socially binding marriage contract.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Spouse of</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/legally-recognized-relationship-with"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/extended-family-of">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">A relationship between two or more persons who are part of the same extended family.</skos:scopeNote>
	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
