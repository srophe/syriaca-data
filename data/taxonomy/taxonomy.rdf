	<skos:inScheme rdf:resource="http://syriaca.org/taxonomy/syriac-taxonomy"/>
	<skos:prefLabel xml:lang="en">Carrier of Letter to</skos:prefLabel>
	<skos:broader rdf:resource="http://syriaca.org/taxonomy/professional-relationship"/>
</rdf:Description>

<rdf:Description rdf:about="http://syriaca.org/taxonomy/colleague-of">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
