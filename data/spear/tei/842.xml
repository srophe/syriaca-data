            <ab type="factoid" subtype="event" xml:id="factoid-9" resp="#jwickes">
                <idno type="URI">http://syriaca.org/spear/842-9</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/wandering">
                        <desc><persName ref="http://syriaca.org/person/2032"> Kashish </persName>
                            left the <placeName ref="http://syriaca.org/place/399"> Monastery of
                                John Urtaya </placeName> and travelled to <placeName
                                ref="http://syriaca.org/place/715"> Egypt, </placeName><placeName
                                ref="http://syriaca.org/place/2847"> Thebais, </placeName><placeName
                                ref="http://syriaca.org/place/2673"> Greece, </placeName><placeName
                                ref="http://syriaca.org/place/2797"> Illyricum,
                                </placeName><placeName ref="http://syriaca.org/place/641"> Rome,
                            </placeName> and other countries. </desc>
                    </event>
                </listEvent>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/work/842"/>
                    <citedRange unit="p">506</citedRange>
