                    <person>
                        <persName ref="http://syriaca.org/person/2032"/>
                        <trait type="gender" ana="http://syriaca.org/keyword/male">
                            <note type="desc"><persName ref="http://syriaca.org/person/2032">Kashish</persName> was male.</note>
                        </trait>
                    </person>
                </listPerson>
                <bibl type="primary"><ptr target="http://syriaca.org/work/842"/><citedRange unit="pp">505</citedRange></bibl>
