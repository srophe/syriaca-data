                        <occupation ana="http://syriaca.org/keyword/anchorites">
                            <note type="desc"><persName ref="http://syriaca.org/person/50"
                                    >John</persName> distinguished himself in the divine practice of
                                the solitary life. </note>
                        </occupation>
                    </person>
                </listPerson>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/work/816"/>
