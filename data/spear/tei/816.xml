                <idno type="URI">http://syriaca.org/spear/816-17</idno>
                <listPerson>
                    <person>
                        <persName ref="http://syriaca.org/person/74">John</persName>
                    </person>
                </listPerson>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/work/816"/>
                    <citedRange unit="p">320</citedRange>
                </bibl>
                <bibl type="primary">
