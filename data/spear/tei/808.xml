            <ab type="factoid" subtype="event" xml:id="factoid-5" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-5</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/anchorites">
                        <desc><persName ref="http://syriaca.org/person/2097">Simeon</persName> lived
                            in the mountains like the wild beasts. </desc>
                    </event>
                </listEvent>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">229</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">229</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-6" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-6</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/visions">
                        <desc><persName ref="http://syriaca.org/person/2097">Simeon</persName> would
                            preach while in a state of ecstatic contemplation </desc>
                    </event>
                </listEvent>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">229-231</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">229-231</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="nameVariant" xml:id="factoid-8" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-8</idno>
                <listPerson>
                    <personGrp>
                        <persName ref="http://syriaca.org/person/3699">The shepherds who were
                            addressed by <persName ref="http://syriaca.org/person/2097"> Simeon the
                                mountaineer </persName></persName>
                    </personGrp>
                </listPerson>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">232-233</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">232-233</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="residence" xml:id="factoid-9" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-9</idno>
                <listPerson>
                    <personGrp>
                        <persName ref="http://syriaca.org/person/3699"/>
                        <residence><placeName ref="http://syriaca.org/place/2864"/><note type="desc"><persName ref="http://syriaca.org/person/3699">Anonymi
                                    3699</persName> lived in <placeName ref="http://syriaca.org/place/2864"> 'bdhyr </placeName> on the
                                    <placeName ref="http://syriaca.org/place/82">Euphrates</placeName></note></residence>
                    </personGrp>
                </listPerson>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">232-233</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">232-233</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-10" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-10</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/ritual">
                        <desc><persName ref="http://syriaca.org/person/2097">Simeon</persName>
                            questioned <persName ref="http://syriaca.org/person/3699">the
                                shepherds</persName> about their faith and Christian practices, only
                            to learn that they had neglected their faith. </desc>
                    </event>
                </listEvent>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">232-235</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">232-235</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-11" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-11</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/ritual">
                        <desc><persName ref="http://syriaca.org/person/2097">Simeon</persName>
                            resolved to stay with <persName ref="http://syriaca.org/person/3699">the
                                shepherds</persName> and teach them how to worship God as Christians
                        </desc>
                    </event>
                </listEvent>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">236</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">236</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-12" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-12</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/ritual">
                        <desc><persName ref="http://syriaca.org/person/2097">Simeon</persName>, in
                            his wanderings, came upon a village in <placeName ref="http://syriaca.org/place/2823"> Mʿrbnʾ</placeName> and found
                            their church abandoned and their faith neglected. </desc>
                    </event>
                </listEvent>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">236</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">236</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-15" resp="#ktorabi #dschwartz">
                <idno type="URI">http://syriaca.org/spear/808-15</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/churches">
                        <desc><persName ref="http://syriaca.org/person/2097">Simeon</persName>
                            decided to take up residence in the abandoned church in <placeName ref="http://syriaca.org/place/2823"> Mʿrbnʾ </placeName></desc>
                    </event>
                </listEvent>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">237</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">237</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-16" resp="#ktorabi #dschwartz">
                <idno type="URI">http://syriaca.org/spear/808-16</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/mission">
                        <desc><persName ref="http://syriaca.org/person/2097">Simeon</persName> asked
                            the people of the hamlet to gather around him so that he could
                            proselytize and admonish them for neglecting their faith. </desc>
                    </event>
                </listEvent>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">237-241</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">237-241</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-17" resp="#ktorabi #dschwartz">
                <idno type="URI">http://syriaca.org/spear/808-17</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/communion">
                        <desc><persName ref="http://syriaca.org/person/2097">Simeon</persName> made
                            the oblation and communicated with the people of <placeName ref="http://syriaca.org/place/2823"> Mʿrbnʾ </placeName></desc>
                    </event>
                </listEvent>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">241</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">241</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-18" resp="#ktorabi #dschwartz">
                <idno type="URI">http://syriaca.org/spear/808-18</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/ordination">
                        <desc><persName ref="http://syriaca.org/person/2097">Simeon</persName>
                            called all of the children to him, divided them into two groups: the
                            first was shut away in a room and the children in the second group,
                            consisting of <persName ref="http://syriaca.org/person/3700">eighteen
                                boys and twelve girls</persName>, were initiated into the clerical
                            state by being tonsured. </desc>
                    </event>
                </listEvent>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">243</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">243</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="nameVariant" xml:id="factoid-20" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-20</idno>
                <listPerson>
                    <person>
                        <persName ref="http://syriaca.org/person/3701"> A man who assisted in
                            tonsuring the children marked by <persName ref="http://syriaca.org/person/2097"> Simeon the mountaineer
                            </persName></persName>
                        <!-- Add the correct keyword -->
                    </person>
                </listPerson>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">242-243</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">242-243</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="gender" xml:id="factoid-21" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-21</idno>
                <listPerson>
                    <person>
                        <persName ref="http://syriaca.org/person/3701"/>
                        <trait type="gender" ana="http://syriaca.org/keyword/male">
                            <note type="desc"><persName ref="http://syriaca.org/person/3701">Anonymous 3701</persName> was male.</note>
                        </trait>
                    </person>
                </listPerson>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">242-243</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">242-243</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="relation" xml:id="factoid-22" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-22</idno>
                <listRelation>
                    <relation type="person" ref="http://syriaca.org/keyword/professional-relationship" mutual="http://syriaca.org/person/2097 http://syriaca.org/person/3701"><desc><persName ref="http://syriaca.org/person/2097">Simeon the Mountaineer</persName> and <persName ref="http://syriaca.org/person/3701">Anonymi 3701</persName> had a professional relationship.</desc></relation>
                </listRelation>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">242-243</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">242-243</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="relation" xml:id="factoid-23" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-23</idno>
                <listRelation>
                    <relation type="person" ref="http://syriaca.org/keyword/ordained" active="http://syriaca.org/person/2097" passive="http://syriaca.org/person/3700"><desc><persName ref="http://syriaca.org/person/2097">Simeon the Mountaineer</persName> ordained <persName ref="http://syriaca.org/person/3700">Anonymi 3700</persName>.</desc></relation>
                </listRelation>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">242-243</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">242-243</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-24" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-24</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/ordination">
                        <desc> The parents at first opposed then accepted the tonsuring of their
                            children. </desc>
                    </event>
                </listEvent>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">243-244</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">243-244</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="nameVariant" xml:id="factoid-25" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-25</idno>
                <listPerson>
                    <personGrp>
                        <persName ref="http://syriaca.org/person/3702"> The parents of the <persName ref="http://syriaca.org/person/3700">children</persName> blessed and
                            tonsured by <persName ref="http://syriaca.org/person/2097"> Simeon the
                                mountaineer </persName></persName>
                    </personGrp>
                </listPerson>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">243-244</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">243-244</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-27" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-27</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/death http://syriaca.org/keyword/miracles">
                        <desc><persName ref="http://syriaca.org/person/3602">Two children</persName>
                            were were struck down with illness and died when their <persName ref="http://syriaca.org/person/3601">parents</persName> refused
                                <persName ref="http://syriaca.org/person/2097">Simeon</persName>'s
                            attempt to tonsure them. </desc>
                    </event>
                </listEvent>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">244-245</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">244-245</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="nameVariant" xml:id="factoid-28" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-28</idno>
                <listPerson>
                    <personGrp>
                        <persName ref="http://syriaca.org/person/3602"> Two children struck dead
                            when their <persName ref="http://syriaca.org/person/3601">parents</persName> opposed their ordination by <persName ref="http://syriaca.org/person/2097"> Simeon the mountaineer
                            </persName></persName>
                    </personGrp>
                </listPerson>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">244-245</citedRange></bibl>
                <bibl type="primary"><ptr target="http://syriaca.org/bibl/1409"/><citedRange unit="p">244-245</citedRange></bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-29" resp="#ktorabi">
                <idno type="URI">http://syriaca.org/spear/808-29</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/ritual">
                        <desc><persName ref="http://syriaca.org/person/2097"> Simeon </persName>
                            received no further opposition, so that he continued to select and
                            tonsure children, and within four or five years his church flourished.
                        </desc>
