                        <state type="sanctity" ana="http://syriaca.org/keyword/saints">
                            <note type="desc"><persName ref="http://syriaca.org/person/2097">Simeon</persName> was a saint.</note>
                        </state>
                    </person>
                </listPerson>
                <bibl type="primary"><ptr target="http://syriaca.org/work/808"/><citedRange unit="p">229</citedRange></bibl>
