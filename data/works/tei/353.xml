            <bibl xml:id="work-353" ana="#hagiographic" type="lawd:ConceptualWork">
                <title xml:id="name353-1" xml:lang="syr" source="#bib353-1" syriaca-tags="#syriaca-headword">ܐܢܛܘܢܝܣ</title>
