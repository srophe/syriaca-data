                    <bibl xml:id="bib844-7">
                        <editor>E. W. Brooks</editor>
                        <title>ibid.</title>
                        <ptr target="http://syriaca.org/bibl/2033"/>
                        <citedRange>p.179 [525] - 185 [531]</citedRange>
                        <lang>en</lang>
                    </bibl>
                </note>
                <note xml:lang="en" type="MSS" source="#bib250-1">
                    <bibl xml:id="bib844-8">The manuscript witnesses for edition 2 are undetermined.</bibl>
                </note>
