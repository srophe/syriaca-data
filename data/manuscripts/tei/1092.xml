               <msContents>
            <textLang mainLang="syr"/>
            <msItem xml:id="a1" n="1" defective="true">
              <locus to="100" from="1a">Foll. 1a-100</locus>
              
              <title xml:lang="en">The Psalms, according to the Peshitta version,
                        divided, as usual, into <foreign xml:lang="syr">ܡܪ̈ܡܝܬܐ</foreign> and
                           <foreign xml:lang="syr">ܫܘܒ̈ܚܐ</foreign>.</title>
              
              
              
              
              
              <note>The missing portions are: <bibl>Ps. i. 1—vi. 5</bibl>; <bibl>viii. 1—xii.
                           5</bibl>; <bibl>xv. 1—xviii. 49</bibl>; <bibl>xxviii. 1—xxxii. 6</bibl>;
                           <bibl>xxxvi. 12—xxxviii. 3</bibl>; <bibl>ciii. 17—civ. 11</bibl>; and
                           <bibl>cix. 2 —22</bibl>.</note>
            </msItem>
            <msItem xml:id="a2" n="2" defective="true">
              <locus to="105" from="100b">Foll. 100b-105</locus>
              
              <title xml:lang="en">The Canticles.</title>
              
              
              
              
              
              
              <msItem xml:id="b1" n="3" defective="true">
                <locus to="101" from="100b">Foll. 100b-101</locus>
                
                <title xml:lang="en">The first song of
                           <persName>Moses</persName>.</title>
                
                
                
                
                
                
              </msItem>
