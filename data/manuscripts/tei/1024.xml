            <msContents>
              <textLang mainLang="syr"/>
              <msItem xml:id="p2a1" n="1" defective="true">
                <title xml:lang="en">Services for the canonical hours of the ferial
                        days</title>
                <note>There now remain only the canonical hours for Friday and Saturday.</note>
                <msItem xml:id="p2b1" n="2" defective="true">
                  <locus from="16a" to="40"/>
                  <title xml:lang="en">Services for the canonical hours for Friday</title>
                  <msItem xml:id="p2c1" n="3" defective="true">
                    <locus from="16a" to="19"/>
                    <rubric xml:lang="syr">ܛܟܼܣܐ ܕܪܡܫܐ</rubric>
                  </msItem>
                  <msItem xml:id="p2c2" n="4">
                    <locus from="19a" to="24"/>
                    <rubric xml:lang="syr">ܕܠܠܝܐ</rubric>
                  </msItem>
                  <msItem xml:id="p2c3" n="5">
                    <locus from="24a" to="38"/>
