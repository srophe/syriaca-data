          <persName xml:lang="en" xml:id="name5114-1" srophe:tags="#syriaca-headword" resp="http://syriaca.org">Julian (missionary of the Nobades)</persName>
          <persName xml:lang="syr" xml:id="name5114-2" srophe:tags="#syriaca-headword" source="#bib5114-1">ܝܘܠܝܢܐ</persName>
          <idno type="URI">http://syriaca.org/person/5114</idno>
